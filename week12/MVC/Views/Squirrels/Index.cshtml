@model IEnumerable<MVC.Models.Squirrel>

@{
    ViewBag.Title = "Index";
}

<h2>Squirrels</h2>

@foreach (var item in Model)
{
    <div>
        <h1>@item.Name</h1>
        @if(item.TeethSize>2)
        {
            <h3>Those are big teeth</h3>
        }
        else
        {
            <h3>How do you eat</h3>
        }
        <img src=@item.ImageURL/>
    </div>
}



@foreach (var item in Model) {
    <div>
        <div>
            @Html.DisplayFor(modelItem => item.Name)
        </div>
        <div>
            @Html.DisplayFor(modelItem => item.ImageURL)
        </div>
        <div>
            @Html.DisplayFor(modelItem => item.TeethSize)
        </div>
        
    </div>
}


